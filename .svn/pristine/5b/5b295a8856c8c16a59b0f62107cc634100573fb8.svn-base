<script>
    $('.attack_ip').attr('value', getParam("attack_ip"));
    $('.victim_ip').attr('value', getParam("victim_ip"));
    $('.vtype').attr('value', getParam("vtype"));
    $('.extent').attr('value', getParam("extent"));
    $('.ip').attr('value', getParam("ip"));
    $('.date-range-picker').attr('value', URLdecode(getParam("time_stamp")));

    function URLdecode(sStr) {
        return sStr.replace(/\+/g, ' ').replace(/\%2F/g, '\/').replace(/\%20/g, '');
    }

    function getParam(paramName) {
        paramValue = "";
        isFound = false;
        if (this.location.search.indexOf("?") == 0 && this.location.search.indexOf("=") > 1) {
            arrSource = this.location.search.substring(1, this.location.search.length).split("&");
            i = 0;
            while (i < arrSource.length && !isFound) {
                if (arrSource[i].indexOf("=") > 0) {
                    if (arrSource[i].split("=")[0].toLowerCase() == paramName.toLowerCase()) {
                        paramValue = arrSource[i].split("=")[1];
                        isFound = true;
                    }
                }
                i++;
            }
        }
        return paramValue;
    }
    $(".buttonGroup #userAdd").click(function () {
        $("#addUser").show();
        $("#fade").show();
        return false
    });

    $('#user_delete_form').submit(function (e) {
        e.stopPropagation();
        e.preventDefault();
        var id = $("input:checkbox[name='checkbox']:checked").map(function () {
            return $(this).val();
        }).get().join(',');
        $.ajax({
            url: '/SecApt/index.php/Home/Vir/user_delete',
            type: 'POST',
            dataType: 'json',
            data: {'id': id},
            success: function (data) {
                if (data.status == 'success') {
                    $("#delUser").hide();
                    $("#fade").hide();
                    $('#user_delete_form')[0].reset();
                    layer.alert("删除成功");
                    window.location.reload();
                    $(":checked").attr('checked', false);
                } else {
                    $("#delUser").hide();
                    $("#fade").hide();
                    $(":checked").attr('checked', false);
                    layer.alert("不允许删除当前用户");
                }
            }
        });

    });

    $('#addUser_form').submit(function (e) {
        e.stopPropagation();
        e.preventDefault();
        var data = $('#addUser_form').serialize();
        var checkVal = $('#ischeck input').serialize();
        if (checkVal == '') {
            layer.alert("请选择用户组");
            return false
        } else {

            $.ajax({
                type: "POST",
                url: "/SecApt/index.php/Home/Vir/user_add",
                data: data,
                dataType: "json",
                success: function (data) {
                    if (data.status == 'success') {
                        $("#addUser").hide();
                        $("#fade").hide();
                        $('#addUser_form')[0].reset();
                        layer.alert("添加成功");
                        window.location.reload();
                    } else if (data.status == '1') {
                        layer.alert("用户已存在");
                    }
                }
            });
        }

    })

