.circleBoxLeft{ float:left; width:15%; height:250px; border:0px solid red; background:#f5f5f5;border-right:1px solid #dedede; }
.circleBoxLeft ul{ display:inline-block; width:100%; margin:0px; list-style:none; margin-top:44px;}
.circleBoxLeft ul li{ list-style:none;}
.circleBoxLeft ul li a{ display:inline-block; position:relative; width:100%; text-decoration:none; font-size:14px; line-height:66px; padding-left:10px; padding-right:10px; color:#666666; color:#06C; text-align:center;}
.circleBoxLeft ul li a span{ display:none; }
.circleBoxLeft ul li a:hover{ text-decoration:underline; color:#06C;}
.circleBoxLeft ul li a.sel{ background:#ffffff;border-right:1px solid #fff; color:#06C; font-weight:bold;}
.circleBoxLeft ul li a.sel span{ display:inline-block; width:1px; height:60px; background:#ffffff; position:absolute;right:-2px; top:0px;}


.circleBoxRig{ float:left; width:85%;}
.circleBoxRig .circleTit{ width:100%; position:relative; line-height:36px; height:36px;border:0px solid red;box-shadow: 0px 5px 5px #dddddd;}
.circleBoxRig .circleTit div{ width:100%; float:left; text-align:center;}
.circleBoxRig .circleTit div span{ display:inline-block; width:10%; text-align:center;}
.circleBoxRig .circleTit a{ display:inline-block; position:absolute; width:10px; top:0px; text-align:center; font-size:14px; float:left;}
.circleBoxRig .circleTit a i{ font-size:18px;}
.circleBoxRig .circleTit a.left{ left:2px;}
.circleBoxRig .circleTit a.rig{ right:2px;}


.circleBoxRig .circleCon{ width:100%; margin-top:8px;}
.circleBoxRig .circleCon ul{ width:100%; margin:0px;}
.circleBoxRig .circleCon ul li{ display:inline-block; float:left; position:relative; width:10%; height:68px;line-height:68px; text-align:center; }
.circleBoxRig .circleCon ul li.bg0{ background:#fafafa; }
.circleBoxRig .circleCon ul li span{ display:inline-block; position:relative;-moz-border-radius: 50%; /* Gecko browsers */  -webkit-border-radius: 50%; /* Webkit browsers */border-radius:50%;/* W3C syntax*/ cursor:pointer;}
.circleBoxRig .circleCon ul li span.red{ width:30px; height:30px; background:#F33; margin-top:15px;}
.circleBoxRig .circleCon ul li span.red:hover{}
.circleBoxRig .circleCon ul li span.org{ width:15px; height:15px; background:#F93; margin-top:25px;}
.circleBoxRig .circleCon ul li span.org:hover{}

