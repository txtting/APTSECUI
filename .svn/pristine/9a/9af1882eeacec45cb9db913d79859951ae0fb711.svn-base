    background-color: #1b1d29;
    border-top-color: #1b1d29;
}
.nav-list > li.active {
    background-color: #e3e3e4;
}

.skin-1 .nav-list > li.open > a, .skin-1 .nav-list > li.open > a:hover {
    background-color: #12141e;
    color: #95e1f1;
}
.nav-list > li > a:hover::before{ background-color:#95e1f1;}

.skin-1 .nav-list > li > .submenu > li.active > a {
    border-bottom-color: #2f3e44;
    color: #95e1f1;
}
.nav-list > li > a:hover::before {
    background-color: #1b1d29;
    bottom: 0;
    content: "";
    display: block;
    left: 0;
    max-width: 3px;
    overflow: hidden;
    position: absolute;
    top: -1px;
    width: 3px;
}

.nav-list>li>a:hover:before{display:block;content:"";position:absolute;top:-1px;bottom:0;left:0;width:3px;max-width:3px;overflow:hidden;background-color:#1b1d29}
