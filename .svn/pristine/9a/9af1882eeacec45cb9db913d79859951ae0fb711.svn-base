button.aptSearchBtn{ display:inline-block;border:1px solid #428bca; padding:0px; text-indent:0px; text-align:center; padding-left:10px; padding-right:10px; line-height:29px; color:#fff;background:#428bca;-moz-border-radius: 3px; /* Gecko browsers */  -webkit-border-radius: 3px; /* Webkit browsers */border-radius:3px;/* W3C syntax*/}

.aptListBtnGroup{ position:absolute; left:10px; height:32px; top:12px; border:0px solid red;}
